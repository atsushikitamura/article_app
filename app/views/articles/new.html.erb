<div class="">
  <div class="">
    <h3>投稿する</h3>
    <%= form_with model: @article, local: true do |f| %>
      <%= render 'shared/error_messages', model: f.object %>

      <%= f.file_field :image %>
      <%= f.text_field :title, placeholder: "title" %>
      <%= f.text_area :content, placeholder: "text", rows: "10" %>
      <%= f.submit "投稿する" %>
    <% end %>
  </div>
</div>